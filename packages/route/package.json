{
  "name": "@talon/route",
  "version": "1.1.2",
  "description": "encode/decode data to/from routes",
  "homepage": "https://github.com/talon/javascript-registry/packages/route#readme",
  "author": "Talon Poole",
  "license": "MIT",
  "main": "dist/route.js",
  "scripts": {
    "format": "prettier --write **/*.js",
    "docs": "documentation readme lib/**/*.js -f md --section API",
    "test": "jest",
    "build": "rm -rf dist && babel lib --ignore '**/*.test.js' -d dist",
    "watch": "nodemon --exec babel-node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/talon/javascript-registry.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "bugs": {
    "url": "https://github.com/talon/javascript-registry/issues"
  },
  "keywords": [
    "utility",
    "route"
  ],
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1"
  },
  "gitHead": "7789c37acfa5859740323f0c45e09c75082bbc54"
}

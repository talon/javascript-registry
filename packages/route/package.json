{
  "name": "@talon/route",
  "version": "1.1.4",
  "description": "encode/decode data to/from routes",
  "homepage": "https://github.com/talon/javascript-registry/packages/route#readme",
  "author": "Talon Poole",
  "license": "MIT",
  "main": "dist/route.js",
  "scripts": {
    "docs": "gulp docs",
    "test": "gulp test",
    "format": "gulp format",
    "compile": "gulp compile",
    "develop": "gulp develop",
    "build": "gulp distribute"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/talon/javascript-registry.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "jest": {
    "moduleFileExtensions": [
      "md",
      "js",
      "ts",
      "jsx",
      "tsx"
    ],
    "testRegex": [
      ".+\\.test\\.[jt]sx?$",
      "(README|API)\\.md$"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/api/"
    ],
    "transform": {
      "^.+\\.([jt]sx?|md)$": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      [
        "babel-plugin-literate",
        {
          "files": "\\.md$"
        }
      ]
    ]
  },
  "bugs": {
    "url": "https://github.com/talon/javascript-registry/issues"
  },
  "keywords": [
    "utility",
    "route"
  ],
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "babel-jest": "^24.9.0",
    "babel-plugin-literate": "talon/babel-plugin-literate#feature/smarter",
    "documentation": "^12.1.4",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.2.0",
    "gulp-documentation": "^3.2.1",
    "gulp-jest": "^4.0.3",
    "gulp-prettier": "^2.3.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "prettier": "^1.19.1",
    "typedoc": "^0.15.3",
    "typedoc-plugin-markdown": "^2.2.11"
  },
  "gitHead": "1fa7f09f82366ee26fedbc47581ca02678c674ee"
}

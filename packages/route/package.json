{
  "name": "@talon/route",
  "version": "1.1.3",
  "description": "encode/decode data to/from routes",
  "homepage": "https://github.com/talon/javascript-registry/packages/route#readme",
  "author": "Talon Poole",
  "license": "MIT",
  "main": "dist/route.js",
  "scripts": {
    "format": "prettier --write **/*.js",
    "docs": "documentation readme lib/**/*.js -f md --section API > /dev/null",
    "test": "jest",
    "build": "rm -rf dist && babel lib --ignore '**/*.test.js' -d dist",
    "watch": "nodemon --exec babel-node lib/index.js",
    "prepare": "npm run test && npm run docs && npm run build",
    "publish": "git push"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/talon/javascript-registry.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "jest": {
    "moduleFileExtensions": [
      "md",
      "js",
      "ts",
      "jsx",
      "tsx"
    ],
    "testRegex": ".*(README|\\.(test|usage|spec))\\.([jt]sx?|md)$",
    "transform": {
      ".*(README|\\.(test|usage|spec))\\.([jt]sx?|md)$": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ],
    "plugins": [
      "babel-plugin-literate"
    ]
  },
  "bugs": {
    "url": "https://github.com/talon/javascript-registry/issues"
  },
  "keywords": [
    "utility",
    "route"
  ],
  "dependencies": {
    "babel-plugin-literate": "github:talon/babel-plugin-literate#feature/configuration",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1"
  },
  "gitHead": "2402f535fc6791c56cecce2ffb8506e7081323c2"
}
